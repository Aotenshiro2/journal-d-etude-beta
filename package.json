{
  "name": "mon-projet-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:stable": "nodemon --exec \"npm run dev\" --watch src --ext ts,tsx,js,jsx --delay 2500",
    "dev:watch": "nodemon --exec \"next dev\" --watch src --watch prisma --ext ts,tsx,js,jsx,prisma --delay 2500 --verbose",
