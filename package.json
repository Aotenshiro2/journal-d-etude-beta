    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:reset": "prisma db push --force-reset"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@tiptap/react": "^3.6.7",
    "@tiptap/starter-kit": "^3.6.7",
    "@types/uuid": "^11.0.0",
    "@xyflow/react": "^12.8.6",
    "fabric": "^6.7.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.5",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
